
<!DOCTYPE html>
<html lang="vi">
<head>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
</head>
<body>
<div class="container">
    <form style="margin-top: 10%">
        <div class="form-group">
            <label>Mã số thuế</label>
            <input id="ma_so_thue"class="form-control">
      
        </div>
        <div class="form-group">
            <label>Mã tra cứu</label>
            <input  id="ma_tra_cuu" class="form-control">
        </div>
        <button id="look_up" @*onclick="LookupInvoice()"*@ type="submit" class="btn btn-primary">Submit</button>
    </form>


</div>

</body>
</html>
<script src="~/Scripts/jquery-1.10.2.js"></script>
<script src="https://code.jquery.com/jquery-3.3.1.js" ></script>
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.5/jquery.min.js"></script>

<script>
    $(document).ready(function () {
        $("#look_up").click(function (e) {
            e.preventDefault();
            debugger;
            $.ajax({
               
                url: "/Lookup/GetInvoice",
                type: "POST",
                dataType: "json",
				data: { maBaoMat: $("#ma_tra_cuu").val().trim(), maSoThue: $("#ma_so_thue").val().trim() },
                success: function (result) {
                
                    window.location.href = result.redirectToUrl;
                },
                error: function (status) {
                    alert("Failed");
                }
            });
        });
    });
</script>